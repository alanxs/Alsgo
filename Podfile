platform :ios,'8.0'
use_frameworks!

target_arry = ['Alsgo']
target_arry.each do |t|
    target t do
        pod 'Masonry', :git => 'https://github.com/SnapKit/Masonry.git' #自动布局
        pod 'AFNetworking'
        pod 'YYModel'
        pod 'SVProgressHUD', :git => 'https://github.com/SVProgressHUD/SVProgressHUD.git'
    end
end
